---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title type="text" xml:lang="en">unpluggd</title>
    <link type="application/atom+xml" href="http://unplug.gd/feed/" rel="self"/>
    <link type="text" href="http://unplug.gd" rel="alternate"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://unplug.gd</id>
    <author>
        <name>Phillip B Oldham</name>
    </author>
    <rights>Copyright (c) 2010-2011 Phillip B Oldham</rights>
    {% for post in site.posts %}
    <entry>
        <title>{{ post.title | xml_escape }}</title>
        <link href="http://unplug.gd{{ post.url }}/"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://unplug.gd{{ post.url }}/</id>
        <summary type="html">{{ post.excerpt | xml_escape }}</summary>
    </entry>
    {% endfor %}
</feed>
